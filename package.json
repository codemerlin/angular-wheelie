{
  "name": "angular-wheelie",
  "version": "1.0.0",
  "description": "Mousewheel as an angular service",
  "main": "dist/angular-wheelie.js",
  "scripts": {
    "lint": "eslint .",
    "test": "mocha-phantomjs test/index.html",
    "build": "npm run lint && npm test && grunt build",
    "prepublish": "npm run lint && npm test",
    "coverage": "nyc npm test",
    "travis": "nyc --all npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "exclude": [
      "test",
      "dist"
    ],
    "include": [
      "src"
    ]
  },
  "keywords": [
    "angular",
    "wheel",
    "mousewheel"
  ],
  "author": "Joel Mukuthu <joelmukuthu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "^1.5.6",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.1",
    "grunt": "^1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-release-it": "^1.0.1",
    "load-grunt-tasks": "^3.5.0",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "mocha-phantomjs": "^4.0.2",
    "nyc": "^6.4.4",
    "phantomjs-prebuilt": "^2.1.7",
    "sinon": "^1.17.4",
    "unexpected": "^10.13.3",
    "unexpected-sinon": "^10.2.1"
  },
  "dependencies": {
    "angular": "^1.5.6"
  },
  "peerDependencies": {
    "angular": "^1.5.6"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelmukuthu/angular-wheelie.git"
  },
  "bugs": {
    "url": "https://github.com/joelmukuthu/angular-wheelie/issues"
  },
  "homepage": "https://github.com/joelmukuthu/angular-wheelie#readme"
}
